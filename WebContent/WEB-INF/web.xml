<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>TuBiao</display-name>
	<welcome-file-list>
		<welcome-file>tubiao.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>bing</servlet-name>
		<servlet-class>servlet.MainServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bing</servlet-name>
		<url-pattern>/jsp</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>canves</servlet-name>
		<servlet-class>servlet.canves</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>canves</servlet-name>
		<url-pattern>/jsp/ca</url-pattern>
	</servlet-mapping>
</web-app>